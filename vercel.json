{
  "buildCommand": "pnpm turbo run build --filter=@soccer/web",
  "outputDirectory": "apps/web/.next",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "apps/api/dist/index.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "https://api-soccer.vercel.app/trpc/:path*"
    }
  ]
}